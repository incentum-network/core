{
  "enabled": true,
  "port": 4003,
  "versions": {
    "default": 1,
    "valid": [
      1,
      2
    ]
  },
  "cache": {
    "enabled": false,
    "options": {
      "name": "redisCache",
      "engine": "catbox-redis",
      "host": "localhost",
      "port": 6379,
      "partition": "cache",
      "expiresIn": 60000
    }
  },
  "rateLimit": {
    "enabled": false,
    "limit": 300,
    "expires": 60000
  },
  "pagination": {
    "limit": 100,
    "include": [
      "/v2/blocks",
      "/v2/blocks/{id}/transactions",
      "/v2/blocks/search",
      "/v2/delegates",
      "/v2/delegates/{id}/blocks",
      "/v2/delegates/{id}/voters",
      "/v2/multisignatures",
      "/v2/peers",
      "/v2/signatures",
      "/v2/transactions",
      "/v2/transactions/search",
      "/v2/votes",
      "/v2/wallets",
      "/v2/wallets/{id}/transactions",
      "/v2/wallets/{id}/transactions/received",
      "/v2/wallets/{id}/transactions/send",
      "/v2/wallets/{id}/votes",
      "/v2/wallets/search",
      "/v2/webhooks"
    ]
  },
  "statistics": {
    "enabled": false
  }
}
