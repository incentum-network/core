{
  "port": 4002,
  "version": "2.0.0",
  "fastRebuild": true,
  "logging": {
    "driver": "app/logging/winston",
    "options": {
      "file": "debug",
      "console": "info"
    }
  },
  "database": {
    "driver": "app/database/sequelize",
    "options": {
      "uri": "sqlite:storage/database/devnet.sqlite",
      "uri_1": "postgres://node:password@localhost:5432/ark_devnet",
      "dialect": "sqlite",
      "dialect_1": "postgres",
      "logging": false
    }
  },
  "redis": {
    "host": "localhost",
    "port": 6379
  },
  "transactionPool": {
    "enabled": false,
    "key": "ark/pool",
    "maxTransactionsPerSender": 5,
    "whiteList" : []
  },
  "peers": {
    "minimumNetworkReach": 10,
    "blackList": []
  }
}
