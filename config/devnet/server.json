{
  "port": 4002,
  "version": "2.0.0",
  "fastRebuild": false,
  "logging": {
    "file": "debug",
    "console": "info"
  },
  "db": {
    "driver": "app/database/sequelize",
    "options": {
      "ur11": "sqlite:storage/database/testnet.sqlite",
      "uri": "postgres://node:password@localhost:5432/ark_testnet",
      "dialect1": "sqlite",
      "dialect": "postgres",
      "logging": false
    }
  },
  "txpool": {
    "enabled": false,
    "host": "localhost",
    "port": 6379,
    "key": "ark/pool"
  },
  "queue": {
    "host": "localhost",
    "port": 6379
  },
  "peers": {
    "minimumNetworkReach": 10,
    "blackList": []
  }
}
