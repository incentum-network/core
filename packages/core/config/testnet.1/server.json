{
  "test": false,
  "port": 4101,
  "version": "2.0.0",
  "logging": {
    "driver": "@arkecosystem/core-logger-winston",
    "options": {
      "file": "~/.ark/logs/core/testnet",
      "levels": {
        "file": "debug",
        "console": "debug"
      }
    }
  },
  "database": {
    "driver": "@arkecosystem/core-db-sequelize",
    "options": {
      "uri": "sqlite:~/.ark/database/testnet1.sqlite",
      "uri1": "postgres://node:password@localhost:5432/ark_testnet",
      "dialect": "sqlite",
      "dialect1": "postgres",
      "logging": false
    }
  },
  "redis": {
    "host": "localhost",
    "port": 6379
  },
  "transactionPool": {
    "enabled": false,
    "keyPrefix": "ark/pool",
    "maxPoolSize": 10000,
    "maxTransactionsPerSender": 5,
    "whiteList" : []
  },
  "peers": {
    "blackList": [],
    "list": [{
      "ip": "127.0.0.1",
      "port": 4101
    },
    {
      "ip": "127.0.0.1",
      "port": 4102
    }]
  }
}
