{
  "port": 4002,
  "version": "2.0.0",
  "fastRebuild": true,
  "logging": {
    "driver": "@arkecosystem/core-logger-winston",
    "options": {
      "file": "~/.ark/logs/core/devnet",
      "levels": {
        "file": "debug",
        "console": "debug"
      }
    }
  },
  "database": {
    "driver": "@arkecosystem/core-db-sequelize",
    "options": {
      "uri": "sqlite:~/.ark/database/devnet.sqlite",
      "uri_1": "postgres://node:password@localhost:5432/ark_devnet",
      "dialect": "sqlite",
      "dialect_1": "postgres",
      "logging": false
    },
    "snapshots": "~/.ark/snapshots"
  },
  "redis": {
    "host": "localhost",
    "port": 6379
  },
  "transactionPool": {
    "enabled": false,
    "key": "ark/pool",
    "maxTransactionsPerSender": 5,
    "whiteList": []
  },
  "peers": {
    "minimumNetworkReach": 3,
    "list": [{
      "ip": "167.114.29.32",
      "port": 4002
    }, {
      "ip": "167.114.29.33",
      "port": 4002
    }, {
